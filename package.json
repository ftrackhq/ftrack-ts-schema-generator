{
  "name": "@ftrack/ftrack-ts-schema-generator",
  "description": "Gets the schema from a ftrack instance and generates typescript interfaces for all the entity types in the instance.",
  "main": "./dist/ftrack-ts-schema-generator.umd.js",
  "module": "./dist/ftrack-ts-schema-generator.es.js",
  "types": "./dist/ftrack-ts-schema-generator.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:ftrackhq/ftrack-ts-schema-generator.git"
  },
  "scripts": {
    "generate": "ts-node source/index.ts",
    "build": "tsc && vite build"
  },
  "exports": {
    ".": {
      "types": "./dist/ftrack-ts-schema-generator.d.ts",
      "import": "./dist/ftrack-ts-schema-generator.es.js",
      "require": "./dist/ftrack-ts-schema-generator.umd.js"
    }
  },
  "files": ["dist"],
  "dependencies": {
    "@ftrack/api": "1.4.4"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ftrackhq/ftrack-ts-schema-generator/issues"
  },
  "homepage": "http://ftrack.com",
  "devDependencies": {
    "@types/node": "^18.15.7",
    "@types/prettier": "^2.7.2",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.2.0"
  },
  "packageManager": "yarn@3.3.1"
}
