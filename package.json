{
  "name": "@ftrack/ts-schema-generator",
  "description": "Gets the schema from a ftrack instance and generates typescript interfaces for all the entity types in the instance.",
  "main": "dist/ftrack-ts-schema-generator.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ftrackhq/ftrack-ts-schema-generator.git"
  },
  "scripts": {
    "generate": "yarn build && node dist/ftrack-ts-schema-generator.js",
    "build": "esbuild source/index.ts --outfile=dist/ftrack-ts-schema-generator.js --platform=node --bundle --packages=external",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@ftrack/api": "1.4.4"
  },
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ftrackhq/ftrack-ts-schema-generator/issues"
  },
  "homepage": "http://ftrack.com",
  "devDependencies": {
    "@types/node": "^18.15.7",
    "@types/prettier": "^2.7.2",
    "esbuild": "^0.17.14",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "packageManager": "yarn@3.3.1"
}
